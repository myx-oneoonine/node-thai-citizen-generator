{
  "name": "thai-citizen-generator",
  "version": "1.0.3",
  "description": "Thai CitizenId Generator",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "prepare": "npm run build",
    "test": "npm run build && node test/test.js",
    "test:compatibility": "npm run build && node test/compatibility.js",
    "lint": "eslint src/**/*.ts test/*.js",
    "lint:fix": "eslint src/**/*.ts test/*.js --fix",
    "format": "prettier --write src/**/*.ts test/*.js",
    "format:check": "prettier --check src/**/*.ts test/*.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myx-oneoonine/node-thai-citizen-generator.git"
  },
  "author": "myx4play",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/myx-oneoonine/node-thai-citizen-generator/issues"
  },
  "homepage": "https://github.com/myx-oneoonine/node-thai-citizen-generator#readme",
  "devDependencies": {
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.8.3"
  }
}
